(function() {
    var Detail;
    Detail = function(detailData) {
        var win = Ti.UI.createView();
        var amazonPage = 'http://www.amazon.co.jp/dp/'+detailData.asin+'/';
        var webView = Ti.UI.createWebView({
            url:amazonPage
        });
        win.add(webView);
        return win;
    };
    return module.exports = Detail;
})();

